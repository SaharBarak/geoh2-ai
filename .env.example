# H2 Seep Detection - Environment Configuration
# Copy this file to .env and fill in your credentials

# Google Maps API
GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Sentinel Hub
SENTINEL_HUB_CLIENT_ID=your_sentinel_hub_client_id
SENTINEL_HUB_CLIENT_SECRET=your_sentinel_hub_client_secret

# Google Earth Engine
EARTHENGINE_ACCOUNT=your_email@gmail.com

# Optional: Weights & Biases (for experiment tracking)
WANDB_API_KEY=your_wandb_api_key

# Model paths
MODEL_WEIGHTS_PATH=models/weights/best.pt
DATA_DIR=data/
OUTPUT_DIR=outputs/
